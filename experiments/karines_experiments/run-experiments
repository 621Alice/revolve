#!/bin/bash
while true
	do
         for i in {1..10}; do
                ./revolve.py --experiment_name karines_experiments/data/lava_$i --manager experiments/karines_experiments/manager_pop_lava.py --n-cores 5;
                sleep 5s
	            ./revolve.py --experiment_name karines_experiments/data/plane_$i --manager experiments/karines_experiments/manager_pop.py --n-cores 5;
                sleep 5s
	   done
done