 #!/bin/bash


# baseline

while true
	do
         for i in {1..1}; do
                ./revolve.py --experiment-name karines_experiments/data/baseline_$i --run $i --manager experiments/karines_experiments/baseline.py --n-cores 4 --port-start 11400 --world worlds/plane.world --evaluation-time 50;
                sleep 5s
	   done
done






while true
	do
    sleep 1800s;
    kill $(  ps aux | grep 'gzserver' | awk '{print $2}');
    kill $(  ps aux | grep 'revolve.py' | awk '{print $2}');
done





	#./revolve.py --experiment-name karines_experiments/data/tilted5_$i --run $i --manager experiments/karines_experiments/tilted.py --n-cores 4 --port-start 11800 --world worlds/tilted5.world --z-start 0.1 --evaluation-time 50;
