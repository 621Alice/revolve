#!/bin/bash
while true
	do

       for i in {4..10}; do
        ./revolve.py --experiment_name karines_experiments/data/lava_$i --manager experiments/karines_experiments/manager_pop_lava.py --n-cores 5;
        sleep 5s
	    ./revolve.py --experiment_name karines_experiments/data/baseline_$i --manager experiments/karines_experiments/manager_pop.py --n-cores 5;
	    sleep 5s
	   done
done